{
  "name": "react-ts-vite",
  "version": "0.6.0",
  "description": "Boilerplate react js with typescript and vite",
  "private": true,
  "author": "Emmanuel Nipal <emnnipal@gmail.com>",
  "license": "ISC",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "rm -rf build && tsc && vite build",
    "preview": "vite preview",
    "check:types": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "format": "npx prettier --write .",
    "server:start": "cross-env NODE_ENV=production node server/server.js",
    "pm2:start": "pm2 start npm --name 'sample-app-name' -- run server:start",
    "deploy": "npm run build && npm run pm2:start",
    "generate:env": "openssl base64 -A -in .env -out .env.base64",
    "decode:env": "openssl base64 -d -A -in .env.base64 -out .env.output",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.3",
    "@vitest/ui": "^0.24.3",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.55.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.6",
    "immer": "^9.0.16",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "use-immer": "^0.7.0",
    "zod": "^3.19.1",
    "zustand": "4.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
